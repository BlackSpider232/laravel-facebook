{
    "name": "schimpanz/facebook",
    "description": "A Facebook bridge for Laravel",
    "license": "MIT",
    "keywords": ["facebook", "manager", "schimpanz", "laravel"],
    "authors": [
        {
            "name": "Vincent Klaiber",
            "email": "vincent@schimpanz.com"
        }
    ],
    "require": {
        "php": ">=5.5.9",
        "illuminate/contracts": "5.1.* || 5.2.*",
        "illuminate/support": "5.1.* || 5.2.*",
        "graham-campbell/manager": "^2.3",
        "facebook/php-sdk-v4": "^5.1"
    },
    "require-dev": {
        "graham-campbell/testbench": "^3.1",
        "mockery/mockery": "^0.9.4",
        "phpunit/phpunit": "^4.8 || ^5.0"
    },
    "autoload": {
        "psr-4": {
            "Schimpanz\\Facebook\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Schimpanz\\Tests\\Facebook\\": "tests/"
        }
    },
    "config": {
        "preferred-install": "dist"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.2-dev"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
